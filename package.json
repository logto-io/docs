{
  "name": "@logto/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "if test \"$NODE_ENV\" != \"production\" && test \"$CI\" != \"true\" ; then husky install ; fi",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:api": "redoc-cli build https://logto.dev/api/swagger.json -o static/api/index.html --title 'Logto Core API Reference'",
    "swizzle": "docusaurus swizzle",
    "deploy": "pnpm build:api && docusaurus deploy",
    "clear": "rm static/api/index.html && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint --ext ts,tsx,md,mdx src docs i18n",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.13.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/types": "^17.0.0",
    "@docusaurus/core": "2.0.0-beta.22",
    "@docusaurus/module-type-aliases": "2.0.0-beta.22",
    "@docusaurus/preset-classic": "2.0.0-beta.22",
    "@docusaurus/theme-classic": "2.0.0-beta.22",
    "@docusaurus/theme-common": "2.0.0-beta.22",
    "@docusaurus/types": "2.0.0-beta.22",
    "@mdx-js/react": "^1.6.22",
    "@silverhand/eslint-config": "^0.15.0",
    "@silverhand/eslint-config-react": "^0.15.0",
    "@silverhand/ts-config": "^0.15.0",
    "@silverhand/ts-config-react": "^0.15.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.14",
    "clsx": "^1.1.1",
    "core-js": "^3.1.4",
    "docusaurus-plugin-sass": "^0.2.2",
    "eslint": "^8.10.0",
    "eslint-plugin-mdx": "^1.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.6",
    "prettier": "^2.3.2",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^16.8.0",
    "redoc-cli": "^0.13.16",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.8.2",
    "typescript": "^4.6.3",
    "webpack": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": ["@silverhand/react", "plugin:mdx/recommended"],
    "rules": {
      "complexity": "off",
      "@typescript-eslint/prefer-nullish-coalescing": "off",
      "unicorn/filename-case": "off"
    },
    "parserOptions": {
      "extraFileExtensions": ["mdx"]
    }
  },
  "prettier": "@silverhand/eslint-config/.prettierrc"
}
