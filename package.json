{
  "name": "@logto/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "if test \"$NODE_ENV\" != \"production\" && test \"$CI\" != \"true\" ; then husky ; fi",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "cp static/_redirects-docs static/_redirects && pnpm generate-tutorials && docusaurus build",
    "build:tutorials": "cp static/_redirects-tutorials static/_redirects && pnpm generate-tutorials && docusaurus build --config docusaurus-tutorials.config.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "node write-heading-ids.mjs",
    "generate-tutorials": "node generate-tutorials.mjs",
    "lint": "eslint src docs tutorial i18n",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/plugin-client-redirects": "3.7.0",
    "@docusaurus/plugin-content-blog": "3.7.0",
    "@docusaurus/plugin-content-docs": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-classic": "3.7.0",
    "@docusaurus/theme-common": "3.7.0",
    "@docusaurus/theme-mermaid": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@eslint/eslintrc": "^3.1.0",
    "@inkeep/cxkit-react": "^0.5.66",
    "@mdx-js/react": "^3.1.0",
    "@silverhand/eslint-config": "^6.0.0",
    "@silverhand/eslint-config-react": "^6.0.0",
    "@silverhand/essentials": "^2.9.2",
    "@silverhand/ts-config": "^6.0.0",
    "@silverhand/ts-config-react": "^6.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-modal": "^3.16.3",
    "arg": "^5.0.2",
    "clsx": "^2.1.1",
    "core-js": "^3.38.1",
    "docusaurus-plugin-remote-content": "^4.0.0",
    "docusaurus-plugin-sass": "^0.2.6",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-plugin-mdx": "^3.1.5",
    "execa": "^9.5.1",
    "https-proxy-agent": "^7.0.5",
    "husky": "^9.1.6",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.0.0",
    "listr2": "^9.0.0",
    "openai": "^4.73.1",
    "picocolors": "^1.1.1",
    "prettier": "^3.3.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-modal": "^3.16.1",
    "react-paginate": "^8.2.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.2",
    "satori": "^0.14.0",
    "sharp": "^0.34.0",
    "stylelint": "^16.10.0",
    "typescript": "~5.8.0",
    "webpack": "^5.95.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^20.11.0 || ^22.0.0"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc",
  "pnpm": {
    "overrides": {
      "http-proxy-middleware@>=1.3.0 <2.0.9": "^2.0.9",
      "path-to-regexp@<0.1.12": "^0.1.12",
      "webpack-dev-server@<=5.2.0": "^5.2.1",
      "brace-expansion@>=1.0.0 <=1.1.11": "^1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": "^2.0.2"
    },
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure",
      "sharp"
    ]
  }
}
