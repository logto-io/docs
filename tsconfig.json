{
  "extends": "@silverhand/ts-config-react/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@theme/*": [
        "node_modules/@docusaurus/theme-classic/lib/theme/*",
      ]
    },
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic",
      "docusaurus-plugin-sass"
    ],
  },
  "include": ["src", "docs", "*.config.ts"]
}
